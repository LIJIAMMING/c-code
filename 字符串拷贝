#include <assert.h>
void my_strcpy(char* asd, char*fgf)
{
	assert(asd != NULL);//断言
	assert(fgf != NULL);//断言
	while (*asd++ = *fgf++)
	{
		;
	}
}
int main()
{
	//strcpy
	//字符串拷贝
	char arr1[] = "#############";
	char arr2[] = "bit";
	my_strcpy(arr1, arr2);
	printf("%s\n", arr1);
	return 0;
}
