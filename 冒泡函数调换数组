#include <stdio.h>
void bubble_sort(int arr[],int sz)
{
	int i=0;
	for(i=0;i<sz;i++)
	{
		int j=0;
		for(j=0;j<sz-1-i;j++)
		{
			int sum=arr[j];
			arr[j]=arr[j+1];
			arr[j+1]=sum;
		} 
	}
	
	
}

int main()   
{
	int arr[]={9,8,7,6,5,4,3,2,1,0};
	int i=0;
	int sz=sizeof(arr)/sizeof (arr[0]);
	bubble_sort(arr,sz);//冒泡函数 
	for(i=0;i<sz;i++)
	{
		printf("%d ",arr[i]);
	}
	
	return 0;
}
